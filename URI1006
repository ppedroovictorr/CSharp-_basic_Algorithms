using System;

namespace uri1006
{
    class Program
    {
        static void Main(string[] args)
        {
            double A, B, C, MEDIA;

            A = double.Parse(Console.ReadLine());
            B = double.Parse(Console.ReadLine());
            C = double.Parse(Console.ReadLine());

            MEDIA = ((A * 2.0) + (B * 3.0) + (C * 5.0)) / 10;
            Console.WriteLine("MEDIA = " + MEDIA.ToString("F1"));




        }
    }
}
